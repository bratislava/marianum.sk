{
  "extends": "@strapi/typescript-utils/tsconfigs/server",
  "compilerOptions": {
    "outDir": "dist",
    "rootDir": ".",
    "jsx": "react",
    "allowJs": true
  },
  "include": ["./", "src/**/*.json", "./dist/src/*"],
  "exclude": [
    "node_modules/",
    "build/",
    ".cache/",
    ".tmp/",
    "src/admin/",
    "**/*.test.ts",
    "src/plugins/**"
  ]
}
