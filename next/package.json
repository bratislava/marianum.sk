    "@tanstack/react-query": "^5.67.2",
    "clsx": "2.1.1",
    "filesize": "^9.0.11",
    "focus-trap-react": "11.0.3",
    "framer-motion": "^12.26.1",
    "graphql": "16.10.0",
    "graphql-request": "6.1.0",
    "graphql-tag": "2.12.6",
    "i18next": "24.2.2",
    "lodash": "^4.17.21",
    "mapbox-gl": "^2.10.0",
    "meilisearch": "^0.30.0",
    "next": "15.5.9",
    "next-i18next": "15.4.2",
    "next-query-params": "^5.1.0",
    "popmotion": "^11.0.3",
    "prntr": "^2.0.17",
    "react": "19.2.3",
    "react-aria": "3.38.1",
    "react-aria-components": "1.7.1",
