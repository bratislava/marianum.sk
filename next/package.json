    "@graphql-codegen/typescript-operations": "4.5.1",
    "@svgr/webpack": "^6.2.1",
    "@types/lodash": "4.17.16",
    "@types/node": "20.17.28",
    "@types/react": "19.2.8",
    "@types/react-dom": "19.2.3",
    "autoprefixer": "10.4.20",
    "eslint": "^9.39.1",
    "husky": "^8.0.1",
    "i18next-parser": "9.3.0",
    "lint-staged": "^13.0.1",
    "postcss": "8.5.3",
    "prettier": "3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "sass": "^1.57.1",
    "tailwindcss": "3.4.10",
